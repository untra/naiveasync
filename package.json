{
  "name": "@stackhawk/naiveasync",
  "version": "0.3.0",
  "description": "An Opinionated and Painless React & Redux promise wrapper",
  "main": "./dist/src/naiveasync/index.js",
  "types": "./dist/src/naiveasync/index.d.ts",
  "type": "module",
  "author": {
    "name": "Samuel Volin",
    "email": "sam@untra.io",
    "url": "https://github.com/untra"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stackhawk/naiveasync.git"
  },
  "homepage": "https://naiveasync.untra.io/",
  "scripts": {
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run ci && npm run defs",
    "postpublish": "npm run deploy",
    "preversion": "npm run ci",
    "test": "mocha --reporter spec",
    "build": "rm -rf dist && mkdir dist && cp src/index.html dist/index.html && tsc -p build.tsconfig.json",
    "build:watch": "npm run build && tsc -p ./build.tsconfig.json --watch",
    "build:ts": "tsc -p tsconfig.json",
    "build:rs": "./node_modules/.bin/react-scripts build",
    "build:site": "npm run build:ts && npm run build:rs",
    "lint": "tslint --project \"./build.tsconfig.json\"",
    "docs": "typedoc --out docs/docs src",
    "defs": "tsc -p build.tsconfig.json --declaration --outDir dist/ --emitDeclarationOnly",
    "ci": "npm run build && npm run lint",
    "ci2": "npm run lint && npm run build && npm run cover",
    "start": "./node_modules/.bin/react-scripts start",
    "deploy": "./node_modules/.bin/gh-pages -d ./build",
    "predeploy": "npm run build:site",
    "watch": "./node_modules/.bin/webpack-dev-server dist/src/index.js",
    "serve:watch": "./node_modules/.bin/webpack --config webpack.config.js --mode development",
    "cypress:test": "./node_modules/.bin/cypress run --record ",
    "cypress:run": "./node_modules/.bin/cypress run",
    "cypress:open": "./node_modules/.bin/cypress open",
    "build:test": "./node_modules/.bin/webpack --output-filename out.js --entry ./cypress/integration/test.spec.ts"
  },
  "license": "MIT",
  "dependencies": {
    "react": "^16.13.1",
    "redux": "^4.0.5",
    "rxjs": "6.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@cypress/browserify-preprocessor": "^2.2.4",
    "@cypress/webpack-preprocessor": "^4.1.5",
    "@types/chai": "^4.2.11",
    "@types/lodash": "^4.14.151",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.17.21",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-highlight": "0.12.1",
    "@types/react-redux": "^7.1.8",
    "@types/react-router-dom": "4.3.3",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "css-loader": "^3.5.3",
    "cypress": "^4.5.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "gh-pages": "^2.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "7.1.2",
    "react-dom": "^16.13.1",
    "react-highlight": "0.12.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "5.0.1",
    "react-scripts": "^3.4.1",
    "ts-loader": "^6.2.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
